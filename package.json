    "dotenv": "^16.0.0",
    "ngrok": "^4.3.1",
    "node-fetch": "^3.2.4",
    "typescript": "^4.6.4",
    "wrangler": "^0.0.0-62680f6"
  },
  "dependencies": {
    "discord-api-types": "^0.32.0",
    "dishttp": "^2.0.0-alpha.0"
  }
}
