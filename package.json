{
  "name": "chizuru",
  "version": "2.0.0",
  "description": "An HTTP interactions NSFW bot for Discord",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "build": "tsc",
    "dev": "tsc && wrangler dev dist/server.js",
